{
  "kind": "collectionType",
  "collectionName": "perfils_fiscales",
  "info": {
    "singularName": "perfil-fiscal",
    "pluralName": "perfils-fiscales",
    "displayName": "Perfil Fiscal"
  },
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "estado": {
      "type": "enumeration",
      "enum": ["borrador", "completo"],
      "default": "borrador"
    },
    "seccionCompletada": {
      "type": "enumeration",
      "enum": ["A", "B", "C"],
      "default": null
    },
    "progreso": { "type": "integer", "default": 0 },
    "autosave": { "type": "boolean", "default": false },
    "fechaCreacion": { "type": "datetime" },
    "fechaActualizacion": { "type": "datetime" },
    "nombre": { "type": "string" },
    "apellido": { "type": "string" },
    "email": { "type": "email" },
    "cuit": { "type": "string" }
  }
}
